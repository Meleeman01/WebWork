    <!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <title>Wetfish - The best internet on the internet</title>

        <!-- Custom styles -->
        <link rel="stylesheet" href="/css/bundle.css" type="text/css">

        <!-- Custom scripts -->
        <script src="/js/deps.js"></script>
        <script src="/js/bundle.js"></script>
    </head>
    
    <body>
        <div class="stars">
            {{#stars}}
                <div class="{{class}}" style="top:{{top}}; left:{{left}}; transform: rotate({{rotate}});">
                    <img src="/img/sparkle.png" style="width:{{width}}">
                </div>
            {{/stars}}
        </div>

        <div class="clouds">
            <svg class="path" viewBox="543 -138 1337 420" width="1337" height="420" xmlns="http://www.w3.org/2000/svg">
                <style type="text/css">
                    .cloud-path{fill:none;stroke:none;}
                </style>
                
                <path id="path" class="cloud-path" d="M2262.5,583 C2009.5,238,1577.5-2.4,1095.6-2.4 C536.1-2.4,108.7,291-86.3,583" />
            </svg>

            <svg class="cloud" viewBox="543 -138 1337 420" width="1337" height="420">
                <image id="big-cloud" width="100" height="100" xlink:href="big-cloud.png" transform="translate(0, 50)" />
                <image id="cloud-dust" width="100" height="77" xlink:href="cloud-dust.png" transform="translate(0, 100)" />
                <image id="little-cloud" width="135" height="75" xlink:href="little-cloud.png" transform="translate(0, 50)" />

                <animateMotion
                    xlink:href="#big-cloud"
                    dur="50s" 
                    begin="-25s"
                    fill="freeze"
                    rotate="auto-reverse"
                    repeatCount="indefinite">
                    <mpath xlink:href="#path" />
                </animateMotion>

                <animateMotion
                    xlink:href="#cloud-dust"
                    dur="50s" 
                    begin="-30s"
                    fill="freeze"
                    rotate="auto-reverse"
                    repeatCount="indefinite">
                    <mpath xlink:href="#path" />
                </animateMotion>

                <animateMotion
                    xlink:href="#little-cloud"
                    dur="50s" 
                    begin="-10s"
                    fill="freeze"
                    rotate="auto-reverse"
                    repeatCount="indefinite">
                    <mpath xlink:href="#path" />
                </animateMotion>
            </svg>
        </div>

        <div class="atmosphere">
